import java.util.Scanner;

class exercicio12 {

    public static void main(String[] args) {

        Scanner teclado = new Scanner(System.in);

        double altura;
        double pesoidealH;
        double pesoidealF;

        System.out.print("sua altura:");
        altura = teclado.nextDouble();

        pesoidealH = (72.7 * altura) - 58;

        pesoidealF = (62.1 * altura) - 44.7;

        System.out.println("peso ideal para homens:" + pesoidealH);
        System.out.println("peso ideal para mulheres:" + pesoidealF);

    }

}